<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:TimesheetParser">

    <Style x:Key="PanelButtonStyle" TargetType="{x:Type Button}">
        <Setter Property="Width" Value="40" />
        <Setter Property="Height" Value="30" />
        <Setter Property="Margin" Value="5,5,0,5" />
        <Setter Property="Focusable" Value="False" />
    </Style>

    <Style x:Key="LowButtonStyle" TargetType="{x:Type Button}">
        <Setter Property="Margin" Value="5" />
        <Setter Property="Width" Value="100" />
        <Setter Property="Height" Value="20" />
    </Style>

    <SolidColorBrush x:Key="BlackBrush">#000000</SolidColorBrush>
    <SolidColorBrush x:Key="GreenBrush">#4CAF50</SolidColorBrush>
    <SolidColorBrush x:Key="RedBrush">#F44336</SolidColorBrush>
</ResourceDictionary>
